<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.songapp">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SongApp">
        <activity android:name=".Mood"></activity>
        <activity android:name=".Angry" />
        <activity android:name=".Bored" />
        <activity android:name=".Emotional" />
        <activity android:name=".Happy" />
        <activity android:name=".Lonely" />
        <activity android:name=".Loved" />
        <activity android:name=".Motivated" />
        <activity android:name=".Nervous" />
        <activity android:name=".Positive" />
        <activity android:name=".Romantic" />
        <activity android:name=".Sad" />
        <activity android:name=".Sick" />
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>